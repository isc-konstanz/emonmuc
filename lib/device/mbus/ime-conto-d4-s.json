{
    "name": "CONTO D4-S",
    "category": "Meter",
    "group": "IME Messger&auml;te GmbH",
    "description": "<b>Energy Meter CONTO D4-S</b>:<br>Current: 10(63)A<br>Voltage : 230(400) - 240(415)V",

    "driver": "mbus",
    "devices": [
        {
            "description": "Energy Meter",
            "settings": "9600"
        }
    ],

    "options": [
        {
            "id": "serialPort",
            "name": "Serial port",
            "description": "The serial port should be given that connects to the M-Bus converter. (e.g. /dev/ttyS0, /dev/ttyUSB0 on Linux).",
            "type": "text",
            "mandatory": true,
            "syntax": "deviceAddress"
        },
        {
            "id": "mBusAddress",
            "name": "M-Bus address",
            "description": "The M-Bus adress can either be the the primary address or secondary address of the meter.<br>A primary address is specified as integer (e.g. 1 for primary address 1) whereas the secondary address consits of 8 bytes that should be specified in hexadecimal form. (e.g. e30456a6b72e3e4e).<br><br>The <a href='https://www.openmuc.org/m-bus/user-guide/#_wired_m_bus'>jMBus User Guide</a> can be accessed for more detailed description.",
            "type": "text",
            "mandatory": true,
            "syntax": "deviceAddress"
        }
    ],

    "channels": [
        {
            "name": "<node>*energy",
            "description": "Energy",
            "address": "06:fd61",
            "logging": {"loggingInterval": 60000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 60000, "valueType":"DOUBLE", "unit": "kWh"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "<node>"}
                },
                {
                    "process": "kwh_to_power",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "<node>"}
                }
            ]
        }
    ],

    "feeds": [
        {
            "name": "<node>",
            "tag": "Energy",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES",
			"unit": "kWh"
        },
        {
            "name": "<node>",
            "tag": "Power",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES",
			"unit": "W"
        }
    ],

    "syntax": {
        "deviceAddress": {
            "keyValue": false,
            "separator": ":"
        }
    }
}